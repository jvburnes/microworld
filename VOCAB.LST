~
~  VOCAB
~
(PRINT 'VOCAB)

(PACKAGE VOCAB
    (DEF REDP BLUEP LARGEP LONGP)
    (PUT ADJECTIVE-FUNCTION LARGE RED LONG BLUE)
    (PUT FEATURES WEARY TO GIVE COIN GOLD MAN OLD BLUE SAW SAWS HAMMER HAMMERS TOOL TOOLS WRENCH WRENCHES A THE LARGE RED LONG 
    SCREWDRIVER SCREWDRIVERS)
    (PUT SINGULAR-FORM SCREWDRIVERS SAWS HAMMERS TOOLS WRENCHES)
    (PUT INSTANCE TOOL HAMMER SCREWDRIVER SAW WRENCH))

(DEF 'REDP
    '[LAMBDA (OBJECT)
         (EQUAL (GET OBJECT
                    'COLOR)
             'RED)])

(DEF 'BLUEP
    '[LAMBDA (OBJECT)
         (EQ 'BLUE
             (GET OBJECT
                 'COLOR))])

(DEF 'LARGEP
    '[LAMBDA (OBJECT)
         (EQUAL (GET OBJECT
                    'SIZE)
             'LARGE)])

(DEF 'LONGP
    '[LAMBDA (OBJECT)
         (GT (OR (GET OBJECT
                     'LENGTH) 0) 6)])

(PUT 'LARGE
     'LARGEP
     'ADJECTIVE-FUNCTION)

(PUT 'RED
     'REDP
     'ADJECTIVE-FUNCTION)

(PUT 'LONG
     'LONGP
     'ADJECTIVE-FUNCTION)

(PUT 'BLUE
     'BLUEP
     'ADJECTIVE-FUNCTION)

(PUT 'WEARY
     '(ADJECTIVE)
     'FEATURES)

(PUT 'TO
     '(PREPOSITION)
     'FEATURES)

(PUT 'GIVE
     '(VERB)
     'FEATURES)

(PUT 'COIN
     '(NOUN)
     'FEATURES)

(PUT 'GOLD
     '(ADJECTIVE)
     'FEATURES)

(PUT 'MAN
     '(NOUN)
     'FEATURES)

(PUT 'OLD
     '(ADJECTIVE)
     'FEATURES)

(PUT 'BLUE
     '(ADJECTIVE)
     'FEATURES)

(PUT 'SAW
     '(NOUN SINGULAR)
     'FEATURES)

(PUT 'SAWS
     '(NOUN PLURAL)
     'FEATURES)

(PUT 'HAMMER
     '(NOUN SINGULAR)
     'FEATURES)

(PUT 'HAMMERS
     '(NOUN PLURAL)
     'FEATURES)

(PUT 'TOOL
     '(NOUN SINGULAR)
     'FEATURES)

(PUT 'TOOLS
     '(NOUN PLURAL)
     'FEATURES)

(PUT 'WRENCH
     '(NOUN SINGULAR)
     'FEATURES)

(PUT 'WRENCHES
     '(NOUN PLURAL)
     'FEATURES)

(PUT 'A
     '(DETERMINER SINGULAR INDEFINITE)
     'FEATURES)

(PUT 'THE
     '(DETERMINER DEFINITE)
     'FEATURES)

(PUT 'LARGE
     '(ADJECTIVE)
     'FEATURES)

(PUT 'RED
     '(ADJECTIVE)
     'FEATURES)

(PUT 'LONG
     '(ADJECTIVE)
     'FEATURES)

(PUT 'SCREWDRIVER
     '(NOUN SINGULAR)
     'FEATURES)

(PUT 'SCREWDRIVERS
     '(NOUN PLURAL)
     'FEATURES)

(PUT 'SCREWDRIVERS
     'SCREWDRIVER
     'SINGULAR-FORM)

(PUT 'SAWS
     'SAW
     'SINGULAR-FORM)

(PUT 'HAMMERS
     'HAMMER
     'SINGULAR-FORM)

(PUT 'TOOLS
     'TOOL
     'SINGULAR-FORM)

(PUT 'WRENCHES
     'WRENCH
     'SINGULAR-FORM)

(PUT 'TOOL
     '(HAMMER SCREWDRIVER SAW WRENCH)
     'INSTANCE)

(PUT 'HAMMER
     '(H1)
     'INSTANCE)

(PUT 'SCREWDRIVER
     '(S1 S2 S3 S4 S5)
     'INSTANCE)

(PUT 'SAW
     '(SAW1)
     'INSTANCE)

(PUT 'WRENCH
     '(W1 W2)
     'INSTANCE)

